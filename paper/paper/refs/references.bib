  institution = {University of Zurich, Institute for Strategy and Business Economics
	(ISU)},
  year = {2004},
  type = {Working Papers},
  number = {0054},
  month = Mar,
  abstract = {Is free-riding in teams reduced when one member receives a signal
	on his colleague√≠s performance? And how does free-riding depend on
	the signal's type? We address these questions in experimental teams
	in which two agents sequentially exert effort to contribute to the
	team output. We vary the type of information the second mover receives
	prior to his effort choice and find that agents work more when signals
	are available. Overall, behavior differs from predictions of standard
	theory. Signals that are predicted to have no effect are, in fact,
	influential and signals that are predicted to have an effect are
	redundant.},
  keywords = {Team production; Free-riding; Experiment; Information; Signal}
}

